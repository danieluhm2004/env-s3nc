    "@types/node": "^16.7.1",
    "@types/prettier": "^2.3.2",
    "@types/tsc-watch": "4.2.0",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "prettier": "^2.3.2",
    "tsc-watch": "4.5.0",
    "typescript": "^4.3.5"
  }
