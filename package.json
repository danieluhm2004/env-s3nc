{
  "name": "env-s3ync",
  "version": "1.0.0",
  "main": "src/index.js",
  "repository": "https://github.com/danieluhm2004/env-s3nc.git",
  "author": "danieluhm2004 <iam@dan.al>",
  "license": "MIT",
  "scripts": {
    "start": "node .",
    "start:prod": "dotenv -e .env.prod -- node .",
    "start:dev": "dotenv -e .env.dev -- node .",
    "start:stage": "dotenv -e .env.stage -- node .",
    "serve": "yarn clean && dotenv -e .env.dev -- tsc-watch",
    "serve:prod": "yarn clean && dotenv -e .env.prod -- tsc-watch",
    "serve:stage": "yarn clean && dotenv -e .env.stage -- tsc-watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prebuild": "yarn clean",
    "build": "tsc",
    "clean": "rm -rf dist/"
  },
  "dependencies": {
    "@types/commander": "^2.12.2",
    "aws-sdk": "^2.972.0",
    "commander": "^8.1.0",
    "dotenv-cli": "^4.0.0",
    "joi": "^17.4.2"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/eslint": "^7.28.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/node": "^16.4.3",
    "@types/prettier": "^2.3.2",
    "@types/tsc-watch": "4.2.0",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.2",
    "tsc-watch": "4.4.0",
    "typescript": "^4.3.5"
  }
}
