{
  "name": "env-s3nc",
  "description": "It makes it easier to manage environment variables in S3.",
  "version": "1.0.1",
  "main": "dist/src/index.js",
  "bin": "dist/src/index.js",
  "repository": "https://github.com/danieluhm2004/env-s3nc.git",
  "author": "danieluhm2004 <iam@dan.al>",
  "license": "MIT",
  "keywords": [
    "env",
    "environment",
    "s3",
    "aws",
    "bucket",
    "sync",
    "devtools"
  ],
  "scripts": {
    "start": "node .",
    "serve": "yarn clean && tsc-watch",
    "lint": "eslint .",
